{
  "name": "atico/translator-symfony-demo-onedrive-to-xliff",
  "license": "MIT",
  "type": "project",
  "description": "Spreadsheet Translator. Symfony Demo Application. Takes a Microsoft OneDrive spreadsheet file and creates translation files in XLIFF format",
  "autoload": {
    "psr-4": {
      "App\\": "src/App"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.4",
    "samuelvi/spreadsheet-translator-core": "^8.4",
    "samuelvi/spreadsheet-translator-symfony-bundle": "^8.4",
    "samuelvi/spreadsheet-translator-provider-onedrive": "^8.4",
    "samuelvi/spreadsheet-translator-reader-xlsx": "^8.4",
    "samuelvi/spreadsheet-translator-exporter-xliff": "^8.4",
    "symfony/console": "^7.0",
    "symfony/dotenv": "^7.0",
    "symfony/filesystem": "^7.0",
    "symfony/finder": "^7.0",
    "symfony/framework-bundle": "^7.0",
    "symfony/http-foundation": "^7.0",
    "symfony/http-kernel": "^7.0",
    "symfony/routing": "^7.0",
    "symfony/runtime": "^7.0",
    "symfony/security-bundle": "^7.0",
    "symfony/translation": "^7.0",
    "symfony/yaml": "^7.0",
    "symfony/monolog-bundle" : "3.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.0",
    "rector/rector": "^2.1"
  },
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "symfony/runtime": true
    }
  },
  "scripts": {
    "test": "phpunit",
    "rector": "rector process --dry-run",
    "rector-fix": "rector process"
  }
}
